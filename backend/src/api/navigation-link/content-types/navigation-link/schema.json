{
  "kind": "collectionType",
  "collectionName": "navigation_links",
  "info": {
    "singularName": "navigation-link",
    "pluralName": "navigation-links",
    "displayName": "Navigation Link",
    "description": "Additional navigation links for header/footer"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "label": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string",
      "required": true
    },
    "position": {
      "type": "enumeration",
      "enum": ["header", "footer"],
      "required": true,
      "default": "header"
    },
    "order": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "external": {
      "type": "boolean",
      "default": false
    },
    "enabled": {
      "type": "boolean",
      "default": true
    }
  }
}
